{
    "require": {
        "vlucas/phpdotenv": "^5.6",
        "paragonie/constant_time_encoding": "^2.6",
        "fakerphp/faker": "^1.23"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "App\\Models\\": "app/models/",
            "App\\Controllers\\": "app/controllers/"
        }
    },
    "require-dev": {
        "pestphp/pest": "^2.32"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
